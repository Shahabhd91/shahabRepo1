<%-include ../partials/header.ejs%>
    <%-include ../partials/main_nav.ejs%>


        <!--Content for Add page-->

        <div class="container">
            <div class="row">
                <div class="offset-md-3 col-md-6">
                    <h1>
                        <%= title %>
                    </h1>
                    <br>
                    <form class="form" method="POST">
                        <div class="form-group">
                            <label for="nameTextField">busin Title</label>
                            <input type="text" class="form-control" id="nameTextField"
                                placeholder="Enter the busin Title" name="name" required>
                        </div>

                        <div class="form-group">
                            <label for="authorTextField">Number</label>
                            <input type="text" class="form-control" id="authorTextField"
                                placeholder="Enter the busin's Author" name="number" required>
                        </div>

                        <div class="form-group">
                            <label for="publishedTextField">Email</label>
                            <input type="text" class="form-control" id="publishedTextField"
                                placeholder="Enter the year of Publication" name="email" required>
                        </div>
                        <br>
                        <button class="btn btn-primary" type="submit"><i class="fas fa-edit"></i>submit</button>
                        <a href="/businList" class="btn btn-warning"><i class="fas fa-undo"></i>cancel</a>

                    </form>
                </div>
            </div>
        </div>

        <%-include ../partials/bottom_nav.ejs%>
            <%-include ../partials/footer.ejs%>